<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="428.0" prefWidth="508.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.SecondaryController"
      stylesheets="@../css/styles.css">

    <Label fx:id="mainTitle" text="Hello world!" />
    <Label fx:id="subTitle" text="This is a simple demo application." />
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
          <Button fx:id="mainButton" onAction="#buttonClicked" text="Click me!" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
         <Button mnemonicParsing="false" text="Button" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
      </children>
   </AnchorPane>

</VBox>
